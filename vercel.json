{
    "version": 2,
    "builds": [
     {
         "src": "src/api/index.js",
         "use": "@vercel/node"
     }
    ],
    "routes": [
     {
         "src": "/(.*)",
         "dest": "src/api/index.js"
     }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "style-src 'self' 'unsafe-inline' cdnjs.cloudflare.com; script-src 'self' 'unsafe-inline'"
        }
      ]
    }
  ]
 }